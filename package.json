{
  "name": "print-to-pdf",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Modern React application for printing sheets to PDF with advanced layout management",
  "keywords": [
    "react",
    "typescript",
    "pdf",
    "print",
    "layout",
    "figma",
    "cad",
    "design-system",
    "dark-theme"
  ],
  "author": {
    "name": "Alex Dubnyak",
    "email": "alexey.dubnyak@graebert.com",
    "url": "https://github.com/alexdubnyak"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdubnyak/print-to-pdf.git"
  },
  "bugs": {
    "url": "https://github.com/alexdubnyak/print-to-pdf/issues"
  },
  "homepage": "https://github.com/alexdubnyak/print-to-pdf#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json && npm install",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lighthouse": "lhci autorun",
    "prepare": "husky install || true",
    "test": "echo \"No tests yet\" && exit 0",
    "test:watch": "echo \"No tests yet\" && exit 0",
    "test:coverage": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-bundle-analyzer": "^0.7.0",
    "@lhci/cli": "^0.12.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "18.19.0",
    "npm": "10.2.3"
  }
}
